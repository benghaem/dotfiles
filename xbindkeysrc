#Mute
"pamixer --toggle-mute"
  XF86AudioMute

#Increase Volume
"pamixer --increase 5; $HOME/.bin/panel/dzen_vol_not"
  XF86AudioRaiseVolume

#Increase Volume Small
"pamixer --increase 1; $HOME/.bin/panel/dzen_vol_not"
  XF86AudioRaiseVolume + Shift

#Decrease Volume
"pamixer --decrease 5; $HOME/.bin/panel/dzen_vol_not"
  XF86AudioLowerVolume

#Decrease Volume Small
"pamixer --decrease 1; $HOME/.bin/panel/dzen_vol_not"
  XF86AudioLowerVolume + Shift

#Mute Mic
"amixer -q set Capture toggle"
  XF86AudioMicMute

#Start LXrandr
"lxrandr"
  XF86Display

#Caps Lock Indicator
"$HOME/.bin/panel/dzen_caps_not"
  Caps_Lock


